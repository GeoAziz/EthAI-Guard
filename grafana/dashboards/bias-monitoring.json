{
  "dashboard": {
    "title": "EthixAI - Model Bias Monitoring",
    "tags": ["ethixai", "bias", "fairness", "compliance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Bias Alerts Per Hour",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[1h])) by (protected_attribute)",
            "legendFormat": "{{protected_attribute}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Detections"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1h", "now"]},
              "reducer": {"params": [], "type": "sum"},
              "type": "query"
            }
          ],
          "name": "High Bias Detection Rate"
        }
      },
      {
        "id": 2,
        "title": "Fairness Score Trend",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ethixai_aicore_fairness_score",
            "legendFormat": "{{model_type}} - {{protected_attribute}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Fairness Score", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0.6, "colorMode": "critical", "op": "lt", "fill": true, "line": true}
        ]
      },
      {
        "id": 3,
        "title": "Total Bias Detections (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Critical Bias Alerts (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total{severity=\"critical\"}[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Average Fairness Score",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "avg(ethixai_aicore_fairness_score)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value",
          "unit": "percentunit"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Models Below Fairness Threshold",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "count(ethixai_aicore_fairness_score < 0.6)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Bias Distribution by Protected Attribute",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[24h])) by (protected_attribute)",
            "legendFormat": "{{protected_attribute}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Bias Severity Breakdown",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[24h])) by (severity)",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Bias Detection Heatmap (Hourly)",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[1h])) by (protected_attribute)",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "format": "short",
          "label": "Protected Attribute"
        },
        "xAxis": {
          "show": true
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateRdYlGn"
        }
      },
      {
        "id": 10,
        "title": "Top Biased Models",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "topk(10, sum(increase(ethixai_aicore_bias_detections_total[24h])) by (model_type, protected_attribute))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value": "Bias Detections"
              }
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "Fairness Score by Model",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "ethixai_aicore_fairness_score",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value": "Fairness Score"
              }
            }
          }
        ]
      }
    ]
  }
}
