{
  "dashboard": {
    "title": "EthixAI - AI Core Monitoring",
    "tags": ["ethixai", "ai", "ml", "inference"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Inference Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(ethixai_aicore_inference_requests_total[5m])) by (model_type)",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Inferences/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Inference Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(ethixai_aicore_inference_duration_seconds_bucket[5m])) by (le, model_type))",
            "legendFormat": "P50 - {{model_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(ethixai_aicore_inference_duration_seconds_bucket[5m])) by (le, model_type))",
            "legendFormat": "P95 - {{model_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(ethixai_aicore_inference_duration_seconds_bucket[5m])) by (le, model_type))",
            "legendFormat": "P99 - {{model_type}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "Slow Inference"
        }
      },
      {
        "id": 3,
        "title": "SHAP Computation Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ethixai_aicore_shap_computation_duration_seconds_bucket[5m])) by (le, model_type))",
            "legendFormat": "P95 - {{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Model Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "(sum(rate(ethixai_aicore_model_cache_hits_total[5m])) / (sum(rate(ethixai_aicore_model_cache_hits_total[5m])) + sum(rate(ethixai_aicore_model_cache_misses_total[5m])))) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate %", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Bias Detections (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Critical Bias Detections (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(ethixai_aicore_bias_detections_total{severity=\"critical\"}[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Active Inference Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "ethixai_aicore_active_requests",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        }
      },
      {
        "id": 8,
        "title": "Inference Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "(sum(rate(ethixai_aicore_inference_errors_total[5m])) / sum(rate(ethixai_aicore_inference_requests_total[5m]))) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value",
          "unit": "percent"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Dataset Size Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(ethixai_aicore_dataset_size_bucket[5m])) by (le)",
            "format": "heatmap",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
