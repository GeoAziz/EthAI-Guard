{
  "dashboard": {
    "title": "EthixAI - System Health",
    "tags": ["ethixai", "system", "infrastructure", "health"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "orientation": "horizontal",
          "textMode": "name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "UP"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Backend CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"ethixai-backend\"}[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU %", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Backend Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"ethixai-backend\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "AI Core CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"ethixai-ai-core\"}[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU %", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "AI Core Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "ethixai_aicore_memory_usage_bytes{type=\"rss\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Backend Active Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "ethixai_backend_http_requests_in_progress",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 25},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "AI Core Active Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 20},
        "targets": [
          {
            "expr": "ethixai_aicore_active_requests",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Backend Request Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(ethixai_backend_http_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value",
          "unit": "reqps"
        }
      },
      {
        "id": 9,
        "title": "AI Core Inference Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(ethixai_aicore_inference_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal",
          "textMode": "value",
          "unit": "reqps"
        }
      },
      {
        "id": 10,
        "title": "Container Uptime",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "time() - process_start_time_seconds",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "unit": "s"
        }
      },
      {
        "id": 11,
        "title": "Error Rates",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "sum(rate(ethixai_backend_http_errors_total[5m]))",
            "legendFormat": "Backend Errors/s",
            "refId": "A"
          },
          {
            "expr": "sum(rate(ethixai_aicore_inference_errors_total[5m]))",
            "legendFormat": "AI Core Errors/s",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      }
    ]
  }
}
