{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus data source",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
    ],
  "title": "EthAI Day10: Reliability Dashboard",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "Backend p95 latency (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "backend p95"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "AI Core p95 latency (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ai_core_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "ai_core p95"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(http_requests_total[5m]))", "legendFormat": "backend rps" },
        { "expr": "sum(rate(ai_core_http_requests_total[5m]))", "legendFormat": "ai_core rps" }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "type": "timeseries",
      "title": "Error rate (5xx/min)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
          { "expr": "sum(rate(http_requests_total{status=~\u00225..\u0022}[5m]))*60", "legendFormat": "backend 5xx/m" },
          { "expr": "sum(rate(ai_core_http_requests_total{status=~\u00225..\u0022}[5m]))*60", "legendFormat": "ai_core 5xx/m" }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "type": "timeseries",
      "title": "AI Core analyze p95 (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ai_core_analysis_seconds_bucket[5m])) by (le))",
          "legendFormat": "analyze p95"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
    }
  ],
  "schemaVersion": 38,
  "version": 1
}
